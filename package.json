{
  "name": "brew-api",
  "version": "0.0.32",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "lint": "eslint --ignore-path .gitignore .",
    "dev": "nodemon",
    "start": "cross-env NODE_ENV=production node build/src/server.js",
    "build": "tsc && cp -r ./src/schema/*.graphql ./build/src/schema",
    "db:dev": "cross-env NODE_ENV=development sequelize",
    "db:prod": "cross-env NODE_ENV=production sequelize",
    "db:staging": "cross-env NODE_ENV=staging sequelize",
    "db:migrate": "node -r dotenv/config ./node_modules/knex/bin/cli.js migrate:latest --env development",
    "db:rollback": "node -r dotenv/config ./node_modules/knex/bin/cli.js migrate:rollback --env development",
    "db:seed": "node -r dotenv/config ./node_modules/knex/bin/cli.js seed:run --env development",
    "token:gen": "node -r dotenv/config scripts/token.js",
    "test": "jest",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "prettier": "pretty-quick",
    "schema:types": "graphql-codegen --config codegen.yml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dariusbakunas/brew-api.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dariusbakunas/brew-api/issues"
  },
  "homepage": "https://github.com/dariusbakunas/brew-api#readme",
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "@sendgrid/mail": "^6.3.1",
    "@sentry/node": "^5.1.0",
    "apollo-server-express": "^2.4.8",
    "atob": "^2.1.2",
    "btoa": "^1.2.1",
    "cross-env": "^5.2.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-rate-limit": "^3.4.1",
    "graphql": "^14.2.1",
    "graphql-middleware": "^3.0.2",
    "graphql-shield": "^5.3.4",
    "jwks-rsa": "^1.6.0",
    "knex": "^0.19.5",
    "lodash.merge": "^4.6.1",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.5",
    "node-cache": "^4.2.1",
    "nodemailer": "^4.7.0",
    "objection": "^1.6.11",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.10.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^5.7.1",
    "uuid": "^3.3.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@graphql-codegen/cli": "^1.8.1",
    "@graphql-codegen/introspection": "1.8.1",
    "@graphql-codegen/typescript": "1.8.1",
    "@graphql-codegen/typescript-resolvers": "^1.8.1",
    "@types/atob": "^2.1.2",
    "@types/btoa": "^1.2.3",
    "@types/ejs": "^2.6.3",
    "@types/express-rate-limit": "^3.3.3",
    "@types/jest": "^24.0.18",
    "@types/morgan": "^1.7.37",
    "@types/node-fetch": "^2.5.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "copy-webpack-plugin": "^4.6.0",
    "coveralls": "^3.0.3",
    "dotenv": "^6.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.4.1",
    "fast-csv": "^2.4.1",
    "husky": "^3.0.3",
    "jest": "^24.7.1",
    "jsonwebtoken": "^8.5.1",
    "mockdate": "^2.0.2",
    "nodemon": "^1.18.11",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "sequelize-cli": "^5.3.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2"
  }
}
